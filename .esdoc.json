{
  "source": "./lib",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "test": {
          "source": "./test",
          "interfaces": ["test"]
        }
      }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "decorators": true
      }
    }
  ]
}
